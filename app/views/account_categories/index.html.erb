<h1>Listing account_categories</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Name</th>
      <th>Description</th>
      <th>Parent</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @account_categories.each do |account_category| %>
      <tr>
        <td><%= account_category.user_id %></td>
        <td><%= account_category.name %></td>
        <td><%= account_category.description %></td>
        <td><%= account_category.parent_id %></td>
        <td><%= link_to 'Show', account_category %></td>
        <td><%= link_to 'Edit', edit_account_category_path(account_category) %></td>
        <td><%= link_to 'Destroy', account_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Account category', new_account_category_path %>
